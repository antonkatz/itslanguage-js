{
  "presets": [
    [
      "env",
      {
        "loose": true
      }
    ],
    "stage-0"
  ],
  "plugins": [
    "transform-class-properties"
  ],
  "env": {
    "test": {
      "plugins": [
        [
          "istanbul", {
            "exclude": [
              "test/**",
              "**/*.spec.js"
            ]
          }
        ]
      ]
    }
  }
}
