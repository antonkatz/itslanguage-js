{
  "source": "./src",
  "destination": "./esdoc",
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "manual": {
          "index": "./manual/index.md",
          "globalIndex": false,
          "files": [
            "./manual/troubleshooting.md"
          ]
        }
      }
    },
    {
      "name": "esdoc-ecmascript-proposal-plugin",
      "option": {
        "all": true
      }
    }
  ]
}
