{
  "name": "@itslanguage/sdk",
  "version": "4.0.0-beta-10",
  "private": true,
  "description": "The core JavaScript SDK for ITSLanguage.",
  "main": "src/index.js",
  "modules": "src/index.js",
  "files": [
    "LICENSE",
    "package.json",
    "README.md",
    "src/"
  ],
  "engines": {
    "npm": ">=5",
    "node": ">=8"
  },
  "scripts": {
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "preinstall": "$npm_execpath run npmcheckversion",
    "prebuild": "$npm_execpath run build:clean",
    "build": "node ./internals/scripts/build.js",
    "postbuild": "$npm_execpath run esdoc && ncp ./esdoc ./build/docs",
    "build:clean": "rimraf ./build",
    "esdoc": "esdoc -c esdoc.json",
    "esdoc:clean": "rimraf ./esdoc",
    "lint": "$npm_execpath run lint:js",
    "lint:eslint": "eslint --ignore-path .gitignore --fix",
    "lint:js": "$npm_execpath run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "test": "cross-env NODE_ENV=test karma start --single-run --color",
    "test:clean": "rimraf ./coverage",
    "clean:all": "$npm_execpath run build:clean && $npm_execpath run esdoc:clean && $npm_execpath run test:clean"
  },
  "lint-staged": {
    "*.js": [
      "eslint --ignore-path .gitignore --fix",
      "git add"
    ]
  },
  "pre-commit": "lint:staged",
  "author": "ITSLanguage (https://www.itslanguage.nl) <support@d-centralize.nl>",
  "contributors": [
    "Mathijs van den Worm <mathijs@d-centralize.nl>",
    "Jogchum Koerts <jogchum.koerts@itslanguage.nl>",
    "Loek Ehren <loek-ehren@hotmail.com>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itslanguage/itslanguage-js.git"
  },
  "bugs": {
    "url": "https://github.com/itslanguage/itslanguage-js/issues"
  },
  "homepage": "https://github.com/itslanguage/itslanguage-js#readme",
  "dependencies": {
    "autobahn": "0.11.2",
    "debug": "4.1.0",
    "event-emitter": "0.3.5",
    "pcmjs": "0.0.2",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/preset-env": "7.1.0",
    "babel-eslint": "10.0.1",
    "babel-plugin-istanbul": "5.0.1",
    "babelify": "10.0.0",
    "browserify": "16.2.3",
    "cross-env": "5.2.0",
    "esdoc": "1.1.0",
    "esdoc-ecmascript-proposal-plugin": "1.0.0",
    "esdoc-importpath-plugin": "1.0.2",
    "esdoc-standard-plugin": "1.0.0",
    "eslint": "5.7.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsdoc": "3.8.0",
    "fs-extra": "7.0.0",
    "jasmine-ajax": "3.4.0",
    "jasmine-core": "3.2.1",
    "karma": "3.0.0",
    "karma-browserify": "5.3.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "1.1.2",
    "karma-jasmine": "1.1.2",
    "lint-staged": "7.3.0",
    "ncp": "2.0.0",
    "pre-commit": "1.2.2",
    "rimraf": "2.6.2",
    "watchify": "3.11.0"
  }
}
