{
  "name": "itslanguage",
  "private": true,
  "version": "3.0.0-beta-3",
  "description": "The core JavaScript SDK for ITSLanguage.",
  "main": "index.js",
  "engines": {
    "npm": ">=5",
    "node": ">=8"
  },
  "scripts": {
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "preinstall": "npm run npmcheckversion",
    "prebuild": "npm run build:clean",
    "build": "babel src --ignore '**/*.spec.js' --out-dir build --source-maps inline",
    "postbuild": "copy ./packages/itslanguage/**/* ./build/",
    "build:clean": "rimraf ./build/",
    "esdoc": "esdoc -c esdoc.json",
    "esdoc:clean": "rimraf ./esdoc",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-path .gitignore",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "test": "karma start --single-run --color",
    "test:clean": "rimraf ./coverage",
    "clean:all": "npm run build:clean && npm run esdoc:clean && npm run test:clean"
  },
  "lint-staged": {
    "*.js": "lint:eslint"
  },
  "pre-commit": "lint:staged",
  "author": "ITSLanguage (https://www.itslanguage.nl) <support@d-centralize.nl>",
  "contributors": [
    "Mathijs van den Worm <mathijs@d-centralize.nl>",
    "Jogchum Koerts <jogchum.koerts@itslanguage.nl>",
    "Loek Ehren <loek-ehren@hotmail.com>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itslanguage/itslanguage-js.git"
  },
  "bugs": {
    "url": "https://github.com/itslanguage/itslanguage-js/issues"
  },
  "homepage": "https://github.com/itslanguage/itslanguage-js#readme",
  "dependencies": {
    "autobahn": "17.5.2",
    "client-oauth2": "4.1.0",
    "event-emitter": "0.3.5",
    "pcmjs": "0.0.2",
    "uuid": "3.1.0",
    "when": "3.7.8"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-preset-env": "1.6.0",
    "babel-preset-stage-0": "6.24.1",
    "babelify": "7.3.0",
    "browserify": "14.4.0",
    "browserify-istanbul": "2.0.0",
    "copy": "0.3.1",
    "esdoc": "1.0.2",
    "eslint": "4.6.1",
    "eslint-config-d-centralize": "0.4.1",
    "eslint-plugin-async-await": "0.0.0",
    "eslint-plugin-jsdoc": "3.1.2",
    "isparta": "4.0.0",
    "jasmine-ajax": "3.3.1",
    "jasmine-core": "2.8.0",
    "karma": "1.7.1",
    "karma-browserify": "5.1.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.1.0",
    "lint-staged": "4.1.0",
    "pre-commit": "1.2.2",
    "rimraf": "2.6.1",
    "watchify": "3.9.0"
  }
}
